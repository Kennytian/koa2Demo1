{
  "name": "koa2demo1",
  "version": "1.0.7",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=3.0.0"
  },
  "author": "Kenny Tian",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kennytian/koa2Demo1.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "start": "./node_modules/.bin/nodemon $NODE_DEBUG_OPTION index.js",
    "lint": "node_modules/.bin/eslint --ext .js ./ --fix --cache",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.0.1",
    "koa-session": "^5.8.1",
    "md5": "^2.2.1",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nunjucks": "^3.1.3",
    "qs": "^6.5.2",
    "redis": "^2.8.0",
    "sequelize": "^4.37.10"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^1.0.0-rc.8",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintIgnore": [
    "/coverage/**/*.js",
    "/__tests__/**/(*.)(spec|test).js"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "setupFiles": [
      "./__tests__/__config__/setup.js",
      "./__tests__/__config__/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./__tests__/__config__/"
    ],
    "verbose": true
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  }
}
